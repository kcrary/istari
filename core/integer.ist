rewrite /-> Nat.istrue_leqb in Hab/.
so /Nat.minus_leq _ _ (a' + b') (a' + b') Hab ()/ /H/ >> auto.
rewrite /-> Nat.plus_commute in H at 1 2/.
rewrite /<- Nat.minus_assoc in H at all/.
