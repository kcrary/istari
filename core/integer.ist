    {
    rewrite /<- plusz_assoc/.
    reduce //.
    rewrite /-> plusz_id_l/.
    reflexivity.
    }
  apply /IH/.
  right >> split >> auto.
  unfold /ltz/.
  rewrite /<- plusz_assoc/.
  reduce //.
  rewrite /-> plusz_id_l/.
  apply /leqz_refl/.
  }

  {
  rewrite /-> not_leqz in Heq/.
