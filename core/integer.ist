  assert /z`1 +z a <z= z`0/ /Hleq/.
    {
    rewrite /<- not_ltz/.
    intro /H/.
    apply /Hneq/.
    apply /leqz_antisymm/.
      {
      unfold /ltz in H/.
      apply /plusz_cancel_leqz_l _ _ z`1/ >> auto.
      }

      {
      apply /ltz_impl_leqz/ >> auto.
      }
    }
  reduce //.
  typecheck.
    {
    rewrite /<- plusz_assoc/.
    reduce //.
