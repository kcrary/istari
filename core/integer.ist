lemma "Timesz_Leqz"
/
  forall (a a' b b' : Integer) .
    Leqz Zeroz a
    -> Leqz Zeroz b
    -> Leqz a a'
    -> Leqz b b'
    -> Leqz (Timesz a b) (Timesz a' b')
/;

intro /a c b d Ha Hb Hac Hbd/.
infer /a/ /Hofa/.
infer /b/ /Hofb/.
infer /c/ /Hofc/.
infer /d/ /Hofd/.
destruct /a/ /[(a a')]/.
destruct /b/ /[(b b')]/.
destruct /c/ /[(c c')]/.
destruct /d/ /[(d d')]/.
unfold /Timesz at all/.
