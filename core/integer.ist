rewrite /-> Nat.plus_commute in H at 5/.
rewrite /<- Nat.plus_assoc in H at 1 2/.
rewrite /-> Nat.minus_plus_cancel in H/.
  {
  apply /Nat.leq_trans _ (a' * b)/.
