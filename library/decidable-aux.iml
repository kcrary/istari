
functor AfterDecidable () :> sig end =
   struct

      val decidable = parseConstant /Decidable.decidable/ 
      val iff = parseConstant /Logic.iff/ 

      val () =
         RewriteInternal.CICTable.insert 
            RewriteInternal.compatibilityTable
            (decidable, 0, iff)
            (iff, true, andthenOn 0 (Backchain.witnessPriority /decidable_compat_iff _# 3 __/))

   end

